<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_sing"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="wxhnc.Sing">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button812"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        style="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog"
        android:textColor="@android:color/background_dark"
        android:background="@drawable/menu11" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:id="@+id/button1112"
        style="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog"
        android:textColor="@android:color/background_dark"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/button1012"
        android:background="@drawable/menu55" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button62"
        android:background="@drawable/menu3"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button912"
        android:textColor="@color/colorAccent" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button912"
        style="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog"
        android:textColor="@android:color/background_dark"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button812"
        android:background="@drawable/menu22" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button1012"
        style="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog"
        android:textColor="@android:color/background_dark"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button62"
        android:background="@drawable/menu44" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button32"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="36dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/create_room" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:id="@+id/button33"
        android:background="@drawable/sing_lone"
        android:layout_below="@+id/button32"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button34"
        android:layout_marginTop="30dp"
        android:background="@drawable/people"
        android:layout_below="@+id/button33"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/back"
        android:id="@+id/imageView3"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="30dp" />

    <TextView
        android:text="秀出好声音"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView53"
        style="@style/CodeFont"
        android:layout_marginTop="51dp"
        android:layout_below="@+id/button34"
        android:layout_toRightOf="@+id/button62"
        android:layout_toEndOf="@+id/button62" />

</RelativeLayout>
