<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_chat"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
 android:paddingLeft="0dp"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="wxhnc.Chat"
    android:background="@android:color/background_light">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/button1113"
        style="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog"

        android:layout_toEndOf="@+id/button1013"
        android:background="@drawable/menu55" />

    <Button

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button813"
        style="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog"

        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/menu11"/>

    <Button

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button913"
        style="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog"

        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button813"
        android:background="@drawable/menu22"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button913"
        android:id="@+id/button63"
        android:background="@drawable/menu33"
        android:layout_toRightOf="@+id/button913" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button1013"
        style="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog"

        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button63"
        android:background="@drawable/menu4" />

    <Button

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button14"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/search"
        style="@style/Widget.AppCompat.Button.Borderless" />

    <Button

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button13"
        android:layout_below="@+id/button14"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/fs"
        style="@style/Widget.AppCompat.Button.Borderless" />

    <Button

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button12"
        android:layout_below="@+id/button13"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/location"
        style="@style/Widget.AppCompat.Button.Borderless" />

    <Button

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button15"
        android:layout_below="@+id/button12"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/singer"
        style="@style/Widget.AppCompat.Button.Borderless" />

    <Button

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button16"
        android:layout_below="@+id/button15"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/two"
        style="@style/Widget.AppCompat.Button.Borderless" />

    <Button

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button17"
        android:layout_below="@+id/button16"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/message"
        style="@style/Widget.AppCompat.Button.Borderless" />

    <Button

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button29"
        android:layout_below="@+id/button17"
        android:layout_alignParentStart="true"
        android:background="@drawable/chat"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:singleLine="true"
        style="@style/Widget.AppCompat.Button.Borderless" />

</RelativeLayout>
