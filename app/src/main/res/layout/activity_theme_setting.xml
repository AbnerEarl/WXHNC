<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_theme_setting"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="wxhnc.ThemeSetting"
    android:background="@color/common_action_bar_splitter">

    <TextView
        android:text="选择主题颜色："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:id="@+id/textView32"
        android:layout_alignParentTop="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView32"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="11dp"
        android:id="@+id/button70"
        android:background="@android:color/white" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button71"
        android:layout_alignBaseline="@+id/button70"
        android:layout_alignBottom="@+id/button70"
        android:layout_centerHorizontal="true"
        android:background="@android:color/holo_red_light" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button71"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/button72"
        android:background="@android:color/holo_orange_dark" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button70"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="52dp"
        android:id="@+id/button73"
        android:background="@color/colorPrimary" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button73"
        android:layout_alignLeft="@+id/button71"
        android:layout_alignStart="@+id/button71"
        android:id="@+id/button74"
        android:background="@android:color/holo_green_light" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button74"
        android:layout_alignLeft="@+id/button72"
        android:layout_alignStart="@+id/button72"
        android:id="@+id/button75"
        android:background="@color/common_google_signin_btn_text_light_pressed" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button77"
        android:layout_alignLeft="@+id/button75"
        android:layout_alignStart="@+id/button75"
        android:id="@+id/button78" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button77"
        android:layout_alignBaseline="@+id/button76"
        android:layout_alignBottom="@+id/button76"
        android:layout_alignLeft="@+id/button74"
        android:layout_alignStart="@+id/button74" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button80"
        android:layout_alignBaseline="@+id/button79"
        android:layout_alignBottom="@+id/button79"
        android:layout_alignLeft="@+id/button77"
        android:layout_alignStart="@+id/button77" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button79"
        android:layout_marginTop="55dp"
        android:layout_below="@+id/button76"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button81"
        android:layout_alignBaseline="@+id/button80"
        android:layout_alignBottom="@+id/button80"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:id="@+id/button76"
        android:layout_below="@+id/button73"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
